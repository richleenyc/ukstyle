<div class="uk-container uk-padding">
  <%% if notice.present? %>
  <div class="uk-alert-primary" uk-alert>
    <a class="uk-alert-close" uk-close></a>
    <p id="notice"><%%= notice %></p>
  </div>
  <%% end %>

  <h1 class="uk-h3 uk-heading-bullet"><%= plural_table_name.titleize %></h1>

  <ul class="uk-subnav uk-subnav-divider">
    <li>
      <%%= link_to 'New <%= singular_table_name.titleize %>', new_<%= singular_route_name %>_path, class: 'uk-link-muted' %>
    </li>
  </ul>


  <table class="uk-table uk-table-divider uk-table-small">
    <thead>
      <tr>
  <% attributes.reject(&:password_digest?).each do |attribute| -%>
        <th><%= attribute.human_name %></th>
  <% end -%>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <%% @<%= plural_table_name %>.each do |<%= singular_table_name %>| %>
        <tr>
  <% attributes.reject(&:password_digest?).each do |attribute| -%>
          <td><%%= <%= singular_table_name %>.<%= attribute.name %> %></td>
  <% end -%>
          <td><%%= link_to 'Show', <%= model_resource_name %>, class: 'uk-link-muted' %></td>
          <td><%%= link_to 'Edit', edit_<%= singular_route_name %>_path(<%= singular_table_name %>), class: 'uk-link-muted' %></td>
          <td><%%= link_to 'Destroy', <%= model_resource_name %>, method: :delete, class: 'uk-link-muted', data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <%% end %>
    </tbody>
  </table>

</div>